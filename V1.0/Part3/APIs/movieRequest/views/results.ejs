<%- include("partials/header") %>

<h1 class="display-1 jumbotron text-center">Results</h1>
<div class="container">
    <ul>
        <% data.Search.forEach(movie => { %>
        <li class="d-flex justify-content-between align-middle">
            <strong>
                <%= movie.Title %>
            </strong>
            -
            <%= movie.Year %>

            <img class="" src="<%= movie.Poster %>" alt="" class="">
        </li>
        <% }); %>
    </ul>

    <a href="/">back</a>
</div>

</body>
</html>